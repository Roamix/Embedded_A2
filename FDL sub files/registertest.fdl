dp jumper(
  in current:ns(3);
  out next:ns(3)
){
  always{
    next = (next==1) ? 0 : next + 1;
  }
}

dp PC(
  in registerin:ns(5);
  out registerout:ns(5)
){
  reg register:ns(5);
  always{
    register = registerin;
    registerout = register;
    $display("register: " , register);
  }
}

system mysystem{
  PC(registerin, registerout);
  jumper(current, next);
}
